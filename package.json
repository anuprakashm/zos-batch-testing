  "dependencies": {
    "@types/jest": "^25.1.2",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/node": "^13.7.1"
  }
}
