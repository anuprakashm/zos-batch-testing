{
  "name": "zos-batch-testing",
  "version": "1.0.0",
  "description": "zos batch automated testing and jcl scannin",
  "main": "index.js",
  "scripts": {
    "jclcheck:job1": "zowe jck check lf \"zosBatchApplication/badjcl.jcl\"",
    "jclcheck:job2": "zowe jck check lf \"zosBatchApplication/ftp.jcl\"",
    "jclcheck:job3": "zowe jck check lf \"zosBatchApplication/mimcopy.jcl\"",
    "jclcheck:all": "npm run jclcheck:job1 && jclcheck:job2 && jclcheck:job3",
    "test:unit": "jest __tests__/batchUnitTests/*",
    "test:component": "jest __tests__/batchComponentsTests/*",
    "test": "npm run test:unit && npm run test:component"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solsu01/zos-batch-testing.git"
  },
